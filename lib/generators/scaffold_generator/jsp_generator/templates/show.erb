<<%='%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%'%>>
<body>

<% @attributes.each do |att| -%>
<p>
  <b><%= att.name.capitalize %>:</b>
   <%= "${#{@model}.#{att.name}}" %>
</p>
<% end -%>

<a href="<%= path "/${#{@model}.id}/edit" %>">Edit</a>
<a href="<%= path %>">Back</a>

</body>
